#!/bin/sh

BIN_DIR=$(dirname "$0")
if [ "$BIN_DIR" = "." ]
then
    BIN_DIR=$(pwd)
fi

OS=$(uname -s)
if [ "$OS" != "cygwin" ]
then
    exec /usr/bin/jar
fi

# The below separation of work into separate lines and the quoting of the
# variables is very precise. If you change either of them in the slightest,
# it will break.

JAR_BAT=$(cygpath --windows "$BIN_DIR"/jar.bat) 
JAR_CMD=$JAR_BAT\ "$@"
$COMSPEC /c "$JAR_CMD"

